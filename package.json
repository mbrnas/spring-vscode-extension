{
  "name": "spring-documentation-easy-search",
  "displayName": "SpringWing",
  "publisher": "MatijaBrnas",
  "icon": "images/icon.png",
  "description": "Easy search for various Spring documentations online, and Spring snippets",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "spring-documentation-easy-search.springSearch",
        "title": "Spring Search"
      },
      {
        "command": "spring-documentation-easy-search.createCustomClass",
        "title": "Create Java Class File"
    }
    ],
    "snippets": [{
      "language": "java",
      "path": "./snippets.json"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.6.1"
  }
}
